  SubResourceWidth := TexDesc.Width div (UINT(1) shl MipSlice);
  SubResourceHeight := TexDesc.Height div (UINT(1) shl MipSlice);
  TopLeftX := aTopLeftX;
  TopLeftY := aTopLeftY;
  if SubResourceWidth = 0 then // Width := (SubResourceWidth ? SubResourceWidth : 1) - topLeftX;

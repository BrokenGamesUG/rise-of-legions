  Filter := D3D11_FILTER_MIN_MAG_MIP_LINEAR;
  AddressU := D3D11_TEXTURE_ADDRESS_CLAMP;
  AddressV := D3D11_TEXTURE_ADDRESS_CLAMP;
  AddressW := D3D11_TEXTURE_ADDRESS_CLAMP;
  MipLODBias := 0;
  MaxAnisotropy := 1;
  ComparisonFunc := D3D11_COMPARISON_NEVER;
  BorderColor[0] := 1.0;
  BorderColor[1] := 1.0;
  BorderColor[2] := 1.0;
